{
    "name": "realodix/next-project",
    "description": "next-project",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/realodix/next-project",
    "authors": [
        {
            "name": "Budi Hermawan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-dom": "*",
        "phpunit/phpunit": "^7|^8|^9",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.14",
        "phpstan/phpstan": "^0.12.90",
        "realodix/php-cs-fixer-config": "^2.0|^3.0"
    },
    "autoload": {
        "files": [
            "src/bootstrap.php"
        ],
        "psr-4": {
            "Realodix\\NextProject\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Realodix\\NextProject\\Test\\": "tests/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "php-cs-fixer fix -v",
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
