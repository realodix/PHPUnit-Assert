{
    "name": "realodix/next-project",
    "description": "next-project",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/realodix/next-project",
    "authors": [
        {
            "name": "Budi Hermawan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-dom": "*",
        "laminas/laminas-dom": ">=2.7",
        "phpunit/phpunit": "^8.5|^9",
        "symfony/polyfill-php80": "^1.23"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.90",
        "realodix/php-cs-fixer-config": "^2.0|^3.0"
    },
    "autoload": {
        "files": [
            "src/bootstrap.php"
        ],
        "psr-4": {
            "Realodix\\NextProject\\": ["src", "src/Dessert"]
        }
    },
    "autoload-dev": {
        "files": [
            "tests/bootstrap.php"
        ],
        "psr-4": {
            "Realodix\\NextProject\\Test\\": ["tests", "tests/Unit"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "php-cs-fixer fix -v",
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
